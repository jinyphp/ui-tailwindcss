{
    "name": "jiny/tailwindcss",
    "description": "jiny ui components for tailwindcss",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "hojinlee",
            "email": "infohojin@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.3|^8.0",
        "livewire/livewire": "^2.12"
    },
    "autoload": {
        "psr-4": {
            "Jiny\\TailwindCSS\\":"src/"
        },
        "files" : [
            "src/Helpers/Helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Jiny\\TailwindCSS\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jiny\\TailwindCSS\\JinyTailwindCSSServiceProvider"
            ]
        }
    }
}
